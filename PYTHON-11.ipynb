    "# 11.given a positive integer,check if the number is prime or not"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "id": "4f533d7e",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Enter any positive integer8\n",
      "8  number is not Prime\n"
     ]
    }
   ],
   "source": [
    "no=int(input(\"Enter any positive number\"))\n",
    "for i in range(2,int(no/2+1)):\n",
    "    if(no%i==0):\n",
    "        break\n",
    "if(i==int(no/2)):\n",
    "    print(no,\" number is Prime\")\n",
    "else:\n",
    "    print(no,\" number is not Prime\")\n",
    "            "
   ]
