   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Enter Any Suit with Card : spade king\n",
      "Card is Lucky\n"
     ]
    }
   ],
   "source": [
    "c=input(\"Enter Any Suit with Card : \")\n",
    "\n",
    "suit,card=c.split()\n",
    "\n",
    "if(suit=='diamond' or (suit=='spade' and card=='ace') or card=='king'):\n",
    "  print(\"Card is Lucky\")\n",
    "else:\n",
    "  print(\"Please,try next time\")"
   ]
